{
    "name": "symfony-bundles/fork-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Fork Bundle",
    "keywords": ["fork", "flow", "process", "multiprocesses", "thread", "symfony", "bundle"],
    "homepage": "https://github.com/symfony-bundles/fork-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitry Khaperets",
            "email": "khaperets@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "SymfonyBundles\\ForkBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "require": {
        "php": "^7.1.3"
    },
    "require-dev": {
        "symfony/console": "^3.0||^4.0",
        "symfony/yaml": "^2.7||^3.4||^4.0",
        "symfony/framework-bundle": "^2.7||^3.4||^4.0",
        "phpunit/phpunit": "^6.4",
        "phpunit/php-code-coverage": "^5.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    }
}
