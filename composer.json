{
    "name": "symfony-bundles/fork-bundle",
    "type": "symfony-bundle",
    "description": "Symfony Fork Bundle",
    "keywords": ["fork", "flow", "branching", "process", "multiprocesses", "thread", "symfony", "bundle"],
    "homepage": "https://github.com/symfony-bundles/fork-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitry Khaperets",
            "email": "khaperets@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "SymfonyBundles\\ForkBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "require": {
        "php": ">=5.6",
        "symfony/console": "^3.0",
        "symfony/framework-bundle": "~2.7|~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.3"
    },
    "config": {
        "platform": {
            "php": "5.6"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
